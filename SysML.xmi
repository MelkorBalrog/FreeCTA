<ownedComment xmi:id="Systems-Actions-MergeNode-_comment.0" xmi:type="uml:Comment" body="<p>A <code>MergeNode</code> is a <code>ControlNode</code> that asserts the merging of its incoming <code>Successions</code>. A <code>MergeNode</code> may have at most one outgoing <code>Successions</code>.</p>">
<annotatedElement xmi:idref="Systems-Actions-MergeNode"/>
</ownedComment>
<ownedRule xmi:id="Systems-Actions-MergeNode-validateMergeNodeOutgoingSuccessions" xmi:type="uml:Constraint" name="validateMergeNodeOutgoingSuccessions">
<constrainedElement xmi:idref="Systems-Actions-MergeNode"/>
<ownedComment xmi:id="Systems-Actions-MergeNode-validateMergeNodeOutgoingSuccessions-_comment.0" xmi:type="uml:Comment" body="<p>A <code>MergeNode</code> may have at most one outgoing <code>Succession</code>.</p>">
<annotatedElement xmi:idref="Systems-Actions-MergeNode-validateMergeNodeOutgoingSuccessions"/>
</ownedComment>
<specification xmi:id="Systems-Actions-MergeNode-validateMergeNodeOutgoingSuccessions-specification0" xmi:type="uml:OpaqueExpression" body="sourceConnector->selectAsKind(Succession)->size() <= 1" language="OCL2.0" name=""/>
</ownedRule>
<ownedRule xmi:id="Systems-Actions-MergeNode-validateMergeNodeIncomingSuccessions" xmi:type="uml:Constraint" name="validateMergeNodeIncomingSuccessions">
<constrainedElement xmi:idref="Systems-Actions-MergeNode"/>
<ownedComment xmi:id="Systems-Actions-MergeNode-validateMergeNodeIncomingSuccessions-_comment.0" xmi:type="uml:Comment" body="<p>All incoming <code>Successions</code> to a <code>MergeNode</code> must have a source <code>multiplicity</code> of <code>0..1</code>.</p>">
<annotatedElement xmi:idref="Systems-Actions-MergeNode-validateMergeNodeIncomingSuccessions"/>
</ownedComment>
<specification xmi:id="Systems-Actions-MergeNode-validateMergeNodeIncomingSuccessions-specification0" xmi:type="uml:OpaqueExpression" body="targetConnector->selectByKind(Succession)-> collect(connectorEnd->at(1))-> forAll(sourceMult | multiplicityHasBounds(sourceMult, 0, 1))" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-Actions-MergeNode-checkMergeNodeIncomingSuccessionSpecialization" xmi:type="uml:Constraint" name="checkMergeNodeIncomingSuccessionSpecialization">
<constrainedElement xmi:idref="Systems-Actions-MergeNode"/>
<ownedComment xmi:id="Systems-Actions-MergeNode-checkMergeNodeIncomingSuccessionSpecialization-_comment.0" xmi:type="uml:Comment" body="<p>All incoming <code>Successions</code> to a <code>MergeNode</code> must subset the inherited <em><code>incomingHBLink</code></em> <code>feature</code> of the <code>MergeNode</code>.</p>">
<annotatedElement xmi:idref="Systems-Actions-MergeNode-checkMergeNodeIncomingSuccessionSpecialization"/>
</ownedComment>
<specification xmi:id="Systems-Actions-MergeNode-checkMergeNodeIncomingSuccessionSpecialization-specification0" xmi:type="uml:OpaqueExpression" body="targetConnector->selectByKind(Succession)-> forAll(subsetsChain(self, resolveGlobal('ControlPerformances::MergePerformance::incomingHBLink')))" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-Actions-MergeNode-checkMergeNodeSpecialization" xmi:type="uml:Constraint" name="checkMergeNodeSpecialization">
<constrainedElement xmi:idref="Systems-Actions-MergeNode"/>
<ownedComment xmi:id="Systems-Actions-MergeNode-checkMergeNodeSpecialization-_comment.0" xmi:type="uml:Comment" body="<p>A <code>MergeNode</code> must directly or indirectly specialize the <code>ActionUsage</code> <em><code>Actions::Action::merges</code></em> from the Systems Model Library.</p>">
<annotatedElement xmi:idref="Systems-Actions-MergeNode-checkMergeNodeSpecialization"/>
</ownedComment>
<specification xmi:id="Systems-Actions-MergeNode-checkMergeNodeSpecialization-specification0" xmi:type="uml:OpaqueExpression" body="specializesFromLibrary('Actions::Action::merges')" language="OCL2.0" name=""/>
</ownedRule>
</packagedElement>
