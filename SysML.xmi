</ownedComment>
<specification xmi:id="Systems-Actions-TerminateActionUsage-checkTerminateActionUsageSpecialization-specification0" xmi:type="uml:OpaqueExpression" body="specializesFromLibrary('Actions::terminateActions')" language="OCL2.0" name=""/>
</ownedRule>
<ownedRule xmi:id="Systems-Actions-TerminateActionUsage-deriveTerminateActionUsageTerminatedOccurrenceArgument" xmi:type="uml:Constraint" name="deriveTerminateActionUsageTerminatedOccurrenceArgument">
