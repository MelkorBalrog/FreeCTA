<packagedElement xmi:id="Systems-States-TransitionFeatureKind" xmi:type="uml:Enumeration" name="TransitionFeatureKind">
<ownedComment xmi:id="Systems-States-TransitionFeatureKind-_comment.0" xmi:type="uml:Comment" body="<p>A <code>TransitionActionKind</code> indicates whether the <code>transitionFeature</code> of a <code>TransitionFeatureMembership</code> is a trigger, guard or effect.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionFeatureKind"/>
</ownedComment>
<ownedLiteral xmi:id="Systems-States-TransitionFeatureKind-trigger" xmi:type="uml:EnumerationLiteral" name="trigger">
<ownedComment xmi:id="Systems-States-TransitionFeatureKind-trigger-_comment.0" xmi:type="uml:Comment" body="<p>Indicates that the <code>transitionFeature</code> of a <code>TransitionFeatureMembership</code> is a <code>triggerAction</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionFeatureKind-trigger"/>
</ownedComment>
</ownedLiteral>
<ownedLiteral xmi:id="Systems-States-TransitionFeatureKind-guard" xmi:type="uml:EnumerationLiteral" name="guard">
<ownedComment xmi:id="Systems-States-TransitionFeatureKind-guard-_comment.0" xmi:type="uml:Comment" body="<p>Indicates that the <code>transitionFeature</code> of a <code>TransitionFeatureMembership</code> is a <code>guardExpression</code>.</p>">
