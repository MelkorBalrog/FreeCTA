</ownedComment>
<specification xmi:id="Systems-Actions-TerminateActionUsage-deriveTerminateActionUsageTerminatedOccurrenceArgument-specification0" xmi:type="uml:OpaqueExpression" body="terminatedOccurrenceArgument = argument(1)" language="OCL2.0" name=""/>
</ownedRule>
<ownedRule xmi:id="Systems-Actions-TerminateActionUsage-checkTerminateActionUsageSubactionSpecialization" xmi:type="uml:Constraint" name="checkTerminateActionUsageSubactionSpecialization">
