<upperValue xmi:id="Systems-Views-ViewUsage-viewRendering-upperValue" xmi:type="uml:LiteralUnlimitedNatural" name="" value="1"/>
</ownedAttribute>
<ownedAttribute xmi:id="Systems-Views-ViewUsage-viewCondition" xmi:type="uml:Property" isDerived="true" isOrdered="true" name="viewCondition">
<association xmi:idref="Systems-Views-A_viewCondition_owningView"/>
<lowerValue xmi:id="Systems-Views-ViewUsage-viewCondition-lowerValue" xmi:type="uml:LiteralInteger" name=""/>
<ownedComment xmi:id="Systems-Views-ViewUsage-viewCondition-_comment.0" xmi:type="uml:Comment" body="<p>The <code>Expressions</code> related to this <code>ViewUsage</code> by <code>ElementFilterMemberships</code>, which specify conditions on <code>Elements</code> to be rendered in a view.</p>">
<annotatedElement xmi:idref="Systems-Views-ViewUsage-viewCondition"/>
</ownedComment>
<subsettedProperty href="https://www.omg.org/spec/KerML/20250201/KerML.xmi#Root-Namespaces-Namespace-ownedMember"/>
<type href="https://www.omg.org/spec/KerML/20250201/KerML.xmi#Kernel-Functions-Expression"/>
