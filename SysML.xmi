<type href="https://www.omg.org/spec/KerML/20250201/KerML.xmi#Kernel-Functions-Expression"/>
<upperValue xmi:id="Systems-Actions-WhileLoopActionUsage-untilArgument-upperValue" xmi:type="uml:LiteralUnlimitedNatural" name="" value="1"/>
