<annotatedElement xmi:idref="Systems-States-TransitionFeatureMembership-validateTransitionFeatureMembershipGuardExpression"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionFeatureMembership-validateTransitionFeatureMembershipGuardExpression-specification0" xmi:type="uml:OpaqueExpression" body="kind = TransitionFeatureKind::guard implies transitionFeature.oclIsKindOf(Expression) and let guard : Expression = transitionFeature.oclIsKindOf(Expression) in guard.result.specializesFromLibrary('ScalarValues::Boolean') and guard.result.multiplicity <> null and guard.result.multiplicity.hasBounds(1,1)" language="OCL2.0" name=""> </specification>
</ownedRule>
