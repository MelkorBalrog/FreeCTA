</ownedComment>
<specification xmi:id="Systems-States-StateUsage-validateStateUsageStateSubactionKind-specification0" xmi:type="uml:OpaqueExpression" body="ownedMembership-> selectByKind(StateSubactionMembership)-> isUnique(kind)" language="OCL2.0" name=""/>
