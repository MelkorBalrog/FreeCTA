</ownedComment>
<specification xmi:id="Systems-Views-ViewRenderingMembership-validateViewRenderingMembershipOwningType-specification0" xmi:type="uml:OpaqueExpression" body="owningType.oclIsKindOf(ViewDefinition) or owningType.oclIsKindOf(ViewUsage)" language="OCL2.0" name=""/>
</ownedRule>
