<generalization xmi:id="Systems-States-TransitionUsage-_generalization.Systems-Actions-ActionUsage" xmi:type="uml:Generalization">
<general xmi:idref="Systems-Actions-ActionUsage"/>
</generalization>
<ownedAttribute xmi:id="Systems-States-TransitionUsage-source" xmi:type="uml:Property" isDerived="true" name="source">
<association xmi:idref="Systems-States-A_source_outgoingTransition"/>
<lowerValue xmi:id="Systems-States-TransitionUsage-source-lowerValue" xmi:type="uml:LiteralInteger" name="" value="1"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-source-_comment.0" xmi:type="uml:Comment" body="<p>The source <code>ActionUsage</code> of this <code>TransitionUsage</code>, which becomes the <code>source</code> of the <code>succession</code> for the <code>TransitionUsage</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-source"/>
</ownedComment>
<type xmi:idref="Systems-Actions-ActionUsage"/>
<upperValue xmi:id="Systems-States-TransitionUsage-source-upperValue" xmi:type="uml:LiteralUnlimitedNatural" name="" value="1"/>
</ownedAttribute>
<ownedAttribute xmi:id="Systems-States-TransitionUsage-target" xmi:type="uml:Property" isDerived="true" name="target">
<association xmi:idref="Systems-States-A_target_incomingTransition"/>
<lowerValue xmi:id="Systems-States-TransitionUsage-target-lowerValue" xmi:type="uml:LiteralInteger" name="" value="1"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-target-_comment.0" xmi:type="uml:Comment" body="<p>The target <code>ActionUsage</code> of this <code>TransitionUsage<code>, which is the <code>targetFeature</code> of the <code>succession</code> for the <code>TransitionUsage</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-target"/>
</ownedComment>
<type xmi:idref="Systems-Actions-ActionUsage"/>
<upperValue xmi:id="Systems-States-TransitionUsage-target-upperValue" xmi:type="uml:LiteralUnlimitedNatural" name="" value="1"/>
</ownedAttribute>
<ownedAttribute xmi:id="Systems-States-TransitionUsage-triggerAction" xmi:type="uml:Property" isDerived="true" name="triggerAction">
<association xmi:idref="Systems-States-A_triggerAction_triggeredTransition"/>
<lowerValue xmi:id="Systems-States-TransitionUsage-triggerAction-lowerValue" xmi:type="uml:LiteralInteger" name=""/>
<ownedComment xmi:id="Systems-States-TransitionUsage-triggerAction-_comment.0" xmi:type="uml:Comment" body="<p>The <code>AcceptActionUsages</code> that define the triggers of this <code>TransitionUsage</code>, which are the <code>ownedFeatures</code> of the <code>TransitionUsage</code> related to it by <code>TransitionFeatureMemberships</code> with <code>kind = trigger</code>, which must all be <code>AcceptActionUsages</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-triggerAction"/>
</ownedComment>
<subsettedProperty href="https://www.omg.org/spec/KerML/20250201/KerML.xmi#Core-Types-Type-ownedFeature"/>
<type xmi:idref="Systems-Actions-AcceptActionUsage"/>
<upperValue xmi:id="Systems-States-TransitionUsage-triggerAction-upperValue" xmi:type="uml:LiteralUnlimitedNatural" name="" value="-1"/>
</ownedAttribute>
<ownedAttribute xmi:id="Systems-States-TransitionUsage-guardExpression" xmi:type="uml:Property" isDerived="true" name="guardExpression">
<association xmi:idref="Systems-States-A_guardExpression_guardedTransition"/>
<lowerValue xmi:id="Systems-States-TransitionUsage-guardExpression-lowerValue" xmi:type="uml:LiteralInteger" name=""/>
<ownedComment xmi:id="Systems-States-TransitionUsage-guardExpression-_comment.0" xmi:type="uml:Comment" body="<p>The <code>Expressions</code> that define the guards of this <code>TransitionUsage</code>, which are the <code>ownedFeatures</code> of the <code>TransitionUsage</code> related to it by <code>TransitionFeatureMemberships</code> with <code>kind = guard</code>, which must all be <code>Expressions</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-guardExpression"/>
</ownedComment>
<subsettedProperty href="https://www.omg.org/spec/KerML/20250201/KerML.xmi#Core-Types-Type-ownedFeature"/>
<type href="https://www.omg.org/spec/KerML/20250201/KerML.xmi#Kernel-Functions-Expression"/>
<upperValue xmi:id="Systems-States-TransitionUsage-guardExpression-upperValue" xmi:type="uml:LiteralUnlimitedNatural" name="" value="-1"/>
</ownedAttribute>
<ownedAttribute xmi:id="Systems-States-TransitionUsage-effectAction" xmi:type="uml:Property" isDerived="true" name="effectAction">
<association xmi:idref="Systems-States-A_effectAction_activeTransition"/>
<lowerValue xmi:id="Systems-States-TransitionUsage-effectAction-lowerValue" xmi:type="uml:LiteralInteger" name=""/>
<ownedComment xmi:id="Systems-States-TransitionUsage-effectAction-_comment.0" xmi:type="uml:Comment" body="<p>The <code>ActionUsages</code> that define the effects of this <code>TransitionUsage</code>, which are the <code>ownedFeatures</code> of the <code>TransitionUsage</code> related to it by <code>TransitionFeatureMemberships</code> with <code>kind = effect</code>, which must all be <code>ActionUsages</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-effectAction"/>
</ownedComment>
<subsettedProperty href="https://www.omg.org/spec/KerML/20250201/KerML.xmi#Core-Types-Type-feature"/>
<type xmi:idref="Systems-Actions-ActionUsage"/>
<upperValue xmi:id="Systems-States-TransitionUsage-effectAction-upperValue" xmi:type="uml:LiteralUnlimitedNatural" name="" value="-1"/>
</ownedAttribute>
<ownedAttribute xmi:id="Systems-States-TransitionUsage-succession" xmi:type="uml:Property" isDerived="true" name="succession">
<association xmi:idref="Systems-States-A_succession_linkedTransition"/>
<lowerValue xmi:id="Systems-States-TransitionUsage-succession-lowerValue" xmi:type="uml:LiteralInteger" name="" value="1"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-succession-_comment.0" xmi:type="uml:Comment" body="<p>The <code>Succession</code> that is the <code>ownedFeature</code> of this <code>TransitionUsage</code>, which, if the <code>TransitionUsage</code> is triggered, asserts the temporal ordering of the <code>source</code> and <code>target</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-succession"/>
</ownedComment>
<subsettedProperty href="https://www.omg.org/spec/KerML/20250201/KerML.xmi#Root-Namespaces-Namespace-ownedMember"/>
<type href="https://www.omg.org/spec/KerML/20250201/KerML.xmi#Kernel-Connectors-Succession"/>
<upperValue xmi:id="Systems-States-TransitionUsage-succession-upperValue" xmi:type="uml:LiteralUnlimitedNatural" name="" value="1"/>
</ownedAttribute>
<ownedComment xmi:id="Systems-States-TransitionUsage-_comment.0" xmi:type="uml:Comment" body="<p>A <code>TransitionUsage</code> is an <code>ActionUsage</code> representing a triggered transition between <code>ActionUsages</code> or <code>StateUsages</code>. When triggered by a <code>triggerAction</code>, when its <code>guardExpression</code> is true, the <code>TransitionUsage</code> asserts that its <code>source</code> is exited, then its <code>effectAction</code> (if any) is performed, and then its <code>target</code> is entered.</p> <p>A <code>TransitionUsage</code> can be related to some of its <code>ownedFeatures</code> using <code>TransitionFeatureMembership</code> <code>Relationships</code>, corresponding to the <code>triggerAction</code>, <code>guardExpression</code> and <code>effectAction</code> of the <code>TransitionUsage</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage"/>
</ownedComment>
<ownedOperation xmi:id="Systems-States-TransitionUsage-triggerPayloadParameter_" xmi:type="uml:Operation" name="triggerPayloadParameter">
<bodyCondition xmi:id="Systems-States-TransitionUsage-triggerPayloadParameter_-unnamed1" xmi:type="uml:Constraint" name="unnamed1">
<specification xmi:id="Systems-States-TransitionUsage-triggerPayloadParameter_-unnamed1-specification0" xmi:type="uml:OpaqueExpression" body="if triggerAction->isEmpty() then null else triggerAction->first().payloadParameter endif" language="OCL2.0" name=""/>
</bodyCondition>
<ownedComment xmi:id="Systems-States-TransitionUsage-triggerPayloadParameter_-_comment.0" xmi:type="uml:Comment" body="<p>Return the <code>payloadParameter</code> of the <code>triggerAction</code> of this <code>TransitionUsage</code>, if it has one.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-triggerPayloadParameter_"/>
</ownedComment>
<ownedParameter xmi:id="Systems-States-TransitionUsage-triggerPayloadParameter_-" xmi:type="uml:Parameter" name="">
<lowerValue xmi:id="Systems-States-TransitionUsage-triggerPayloadParameter_--lowerValue" xmi:type="uml:LiteralInteger" name=""/>
<type xmi:idref="Systems-DefinitionAndUsage-ReferenceUsage"/>
<upperValue xmi:id="Systems-States-TransitionUsage-triggerPayloadParameter_--upperValue" xmi:type="uml:LiteralUnlimitedNatural" name="" value="1"/>
</ownedParameter>
<ownedRule xmi:idref="Systems-States-TransitionUsage-triggerPayloadParameter_-unnamed1"/>
</ownedOperation>
<ownedOperation xmi:id="Systems-States-TransitionUsage-sourceFeature_" xmi:type="uml:Operation" name="sourceFeature">
<bodyCondition xmi:id="Systems-States-TransitionUsage-sourceFeature_-unnamed1" xmi:type="uml:Constraint" name="unnamed1">
<specification xmi:id="Systems-States-TransitionUsage-sourceFeature_-unnamed1-specification0" xmi:type="uml:OpaqueExpression" body="let features : Sequence(Feature) = ownedMembership-> reject(oclIsKindOf(FeatureMembership)).memberElement-> selectByKind(Feature)-> select(featureTarget.oclIsKindOf(ActionUsage)) in if features->isEmpty() then null else features->first() endif" language="OCL2.0" name=""> </specification>
</bodyCondition>
<ownedComment xmi:id="Systems-States-TransitionUsage-sourceFeature_-_comment.0" xmi:type="uml:Comment" body="<p>Return the <code>Feature</code> to be used as the <code>source</code> of the <code>succession</code> of this <code>TransitionUsage</code>, which is the first <code>member</code> of the <code>TransitionUsage</code> that is a <code>Feature</code>, that is owned by the <code>TransitionUsage</code> via a <code>Membership</code> that is <em>not</em> a <code>FeatureMembership</code>, and whose <code>featureTarget</code> is an <code>ActionUsage</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-sourceFeature_"/>
</ownedComment>
<ownedParameter xmi:id="Systems-States-TransitionUsage-sourceFeature_-" xmi:type="uml:Parameter" name="">
<lowerValue xmi:id="Systems-States-TransitionUsage-sourceFeature_--lowerValue" xmi:type="uml:LiteralInteger" name=""/>
<type href="https://www.omg.org/spec/KerML/20250201/KerML.xmi#Core-Features-Feature"/>
<upperValue xmi:id="Systems-States-TransitionUsage-sourceFeature_--upperValue" xmi:type="uml:LiteralUnlimitedNatural" name="" value="1"/>
</ownedParameter>
<ownedRule xmi:idref="Systems-States-TransitionUsage-sourceFeature_-unnamed1"/>
</ownedOperation>
<ownedRule xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageTarget" xmi:type="uml:Constraint" name="deriveTransitionUsageTarget">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageTarget-_comment.0" xmi:type="uml:Comment" body="<p>The <code>target</code> of a <code>TransitionUsage</code> is given by the <code>featureTarget</code> of the <code>targetFeature</code> of its <code>succession</code>, which must be an <code>ActionUsage</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-deriveTransitionUsageTarget"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageTarget-specification0" xmi:type="uml:OpaqueExpression" body="target = if succession.targetFeature->isEmpty() then null else let targetFeature : Feature = succession.targetFeature->first().featureTarget in if not targetFeature.oclIsKindOf(ActionUsage) then null else targetFeature.oclAsType(ActionUsage) endif endif " language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSpecialization" xmi:type="uml:Constraint" name="checkTransitionUsageSpecialization">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSpecialization-_comment.0" xmi:type="uml:Comment" body="<p>A <code>TransitionUsage</code> must directly or indirectly specialize the <code>ActionUsage</code> <em><code>Actions::transitionActions</code></em> from the Systems Model Library.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-checkTransitionUsageSpecialization"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSpecialization-specification0" xmi:type="uml:OpaqueExpression" body="specializesFromLibrary('Actions::transitionActions')" language="OCL2.0" name=""/>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-validateTransitionUsageSuccession" xmi:type="uml:Constraint" name="validateTransitionUsageSuccession">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-validateTransitionUsageSuccession-_comment.0" xmi:type="uml:Comment" body="<p>A <code>TransitionUsage</code> must have an <code>ownedMember</code> that is a <code>Succession</code> with an <code>ActionUsage</code> as the <code>featureTarget</code> of its <code>targetFeature</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-validateTransitionUsageSuccession"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-validateTransitionUsageSuccession-specification0" xmi:type="uml:OpaqueExpression" body="let successions : Sequence(Successions) = ownedMember->selectByKind(Succession) in successions->notEmpty() and successions->at(1).targetFeature.featureTarget-> forAll(oclIsKindOf(ActionUsage))" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageGuardExpression" xmi:type="uml:Constraint" name="deriveTransitionUsageGuardExpression">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageGuardExpression-_comment.0" xmi:type="uml:Comment" body="<p>The <code>triggerActions</code> of a <code>TransitionUsage</code> are the <code>transitionFeatures</code> of the <code>ownedFeatureMemberships</code> of the <code>TransitionUsage</code> with <code>kind = trigger</code>, which must all be <code>Expressions</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-deriveTransitionUsageGuardExpression"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageGuardExpression-specification0" xmi:type="uml:OpaqueExpression" body="guardExpression = ownedFeatureMembership-> selectByKind(TransitionFeatureMembership)-> select(kind = TransitionFeatureKind::trigger).transitionFeature-> selectByKind(Expression)" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageSource" xmi:type="uml:Constraint" name="deriveTransitionUsageSource">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageSource-_comment.0" xmi:type="uml:Comment" body="<p>The <code>source</code> of a <code>TransitionUsage</code> is <code>featureTarget</code> of the result of <code>sourceFeature()</code>, which must be an <code>ActionUsage</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-deriveTransitionUsageSource"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageSource-specification0" xmi:type="uml:OpaqueExpression" body="source = let sourceFeature : Feature = sourceFeature() in if sourceFeature = null then null else sourceFeature.featureTarget.oclAsType(ActionUsage)" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-validateTransitionUsageTriggerActions" xmi:type="uml:Constraint" name="validateTransitionUsageTriggerActions">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-validateTransitionUsageTriggerActions-_comment.0" xmi:type="uml:Comment" body="<p>If the <code>source</code> of a <code>TransitionUsage</code> is <em>not</em> a <code>StateUsage</code>, then the <code>TransitionUsage</code> must not have any <code>triggerActions</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-validateTransitionUsageTriggerActions"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-validateTransitionUsageTriggerActions-specification0" xmi:type="uml:OpaqueExpression" body="source <> null and not source.oclIsKindOf(StateUsage) implies triggerAction->isEmpty()" language="OCL2.0" name=""/>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageEffectAction" xmi:type="uml:Constraint" name="deriveTransitionUsageEffectAction">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageEffectAction-_comment.0" xmi:type="uml:Comment" body="<p>The <code>effectActions</code> of a <code>TransitionUsage</code> are the <code>transitionFeatures</code> of the <code>ownedFeatureMemberships</code> of the <code>TransitionUsage</code> with <code>kind = effect</code>, which must all be <code>ActionUsages</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-deriveTransitionUsageEffectAction"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageEffectAction-specification0" xmi:type="uml:OpaqueExpression" body="triggerAction = ownedFeatureMembership-> selectByKind(TransitionFeatureMembership)-> select(kind = TransitionFeatureKind::trigger).transitionFeatures-> selectByKind(AcceptActionUsage)" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSuccessionBindingConnector" xmi:type="uml:Constraint" name="checkTransitionUsageSuccessionBindingConnector">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSuccessionBindingConnector-_comment.0" xmi:type="uml:Comment" body="<p>A <code>TransitionUsage</code> must have an <code>ownedMember</code> that is a <code>BindingConnector</code> between its <code>succession</code> and the inherited <code>Feature</code> <code><em>TransitionPerformances::TransitionPerformance::transitionLink</em></code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-checkTransitionUsageSuccessionBindingConnector"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSuccessionBindingConnector-specification0" xmi:type="uml:OpaqueExpression" body="ownedMember->selectByKind(BindingConnector)->exists(b | b.relatedFeatures->includes(succession) and b.relatedFeatures->includes(resolveGlobal( 'TransitionPerformances::TransitionPerformance::transitionLink')))" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSourceBindingConnector" xmi:type="uml:Constraint" name="checkTransitionUsageSourceBindingConnector">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSourceBindingConnector-_comment.0" xmi:type="uml:Comment" body="<p>A <code>TransitionUsage</code> must have an <code>ownedMember</code> that is a <code>BindingConnector</code> between its <code>source</code> and its first input <code>parameter</code> (which redefines <code><em>Actions::TransitionAction::transitionLinkSource</em></code>).</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-checkTransitionUsageSourceBindingConnector"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSourceBindingConnector-specification0" xmi:type="uml:OpaqueExpression" body="ownedMember->selectByKind(BindingConnector)->exists(b | b.relatedFeatures->includes(source) and b.relatedFeatures->includes(inputParameter(1)))" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-checkTransitionUsagePayloadSpecialization" xmi:type="uml:Constraint" name="checkTransitionUsagePayloadSpecialization">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-checkTransitionUsagePayloadSpecialization-_comment.0" xmi:type="uml:Comment" body="<p>If a <code>TransitionUsage</code> has a <code>triggerAction</code>, then the <em><code>payload</code></em> <code>parameter</code> of the <code>TransitionUsage</code> subsets the <code>Feature</code> chain of the <code>triggerAction</code> and its <code>payloadParameter</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-checkTransitionUsagePayloadSpecialization"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-checkTransitionUsagePayloadSpecialization-specification0" xmi:type="uml:OpaqueExpression" body="triggerAction->notEmpty() implies let payloadParameter : Feature = inputParameter(2) in payloadParameter <> null and payloadParameter.subsetsChain(triggerAction->at(1), triggerPayloadParameter())" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-checkTransitionUsageTransitionFeatureSpecialization" xmi:type="uml:Constraint" name="checkTransitionUsageTransitionFeatureSpecialization">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-checkTransitionUsageTransitionFeatureSpecialization-_comment.0" xmi:type="uml:Comment" body="<p>The <code>triggerActions</code>, <code>guardExpressions</code>, and <code>effectActions</code> of a <code>TransitionUsage</code> must specialize, respectively, the <em><code>accepter</code></em>, <em><code>guard</code></em>, and <em><code>effect</code></em> <code>features</code> of the <code>ActionUsage</code> <em><code>Actions::TransitionActions</code></em> from the Systems Model Library.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-checkTransitionUsageTransitionFeatureSpecialization"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-checkTransitionUsageTransitionFeatureSpecialization-specification0" xmi:type="uml:OpaqueExpression" body="triggerAction->forAll(specializesFromLibrary('Actions::TransitionAction::accepter') and guardExpression->forAll(specializesFromLibrary('Actions::TransitionAction::guard') and effectAction->forAll(specializesFromLibrary('Actions::TransitionAction::effect'))" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSuccessionSourceSpecialization" xmi:type="uml:Constraint" name="checkTransitionUsageSuccessionSourceSpecialization">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSuccessionSourceSpecialization-_comment.0" xmi:type="uml:Comment" body="<p>The <code>sourceFeature</code> of the <code>succession</code> of a <code>TransitionUsage</code> must be the <code>source</code> of the <code>TransitionUsage</code> (i.e., the first <code>connectorEnd</code> of the <code>succession</code> must have a <code>ReferenceSubsetting</code> <code>Relationship</code> with the <code>source</code>).</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-checkTransitionUsageSuccessionSourceSpecialization"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-checkTransitionUsageSuccessionSourceSpecialization-specification0" xmi:type="uml:OpaqueExpression" body="succession.sourceFeature = source" language="OCL2.0" name=""/>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-checkTransitionUsageStateSpecialization" xmi:type="uml:Constraint" name="checkTransitionUsageStateSpecialization">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-checkTransitionUsageStateSpecialization-_comment.0" xmi:type="uml:Comment" body="<p>A composite <code>TransitionUsage</code> whose <code>owningType</code> is a <code>StateDefinition</code> or <code>StateUsage</code> and whose <code>source</code> is a <code>StateUsage</code> must directly or indirectly specialize the <code>ActionUsage</code> <em><code>States::StateAction::stateTransitions</code></em> from the Systems Model Library</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-checkTransitionUsageStateSpecialization"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-checkTransitionUsageStateSpecialization-specification0" xmi:type="uml:OpaqueExpression" body="isComposite and owningType <> null and (owningType.oclIsKindOf(StateDefinition) or owningType.oclIsKindOf(StateUsage)) and source <> null and source.oclIsKindOf(StateUsage) implies specializesFromLibrary('States::StateAction::stateTransitions') " language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageSuccession" xmi:type="uml:Constraint" name="deriveTransitionUsageSuccession">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageSuccession-_comment.0" xmi:type="uml:Comment" body="<p>The <code>succession</code> of a <code>TransitionUsage</code> is its first <code>ownedMember</code> that is a <code>Succession</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-deriveTransitionUsageSuccession"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageSuccession-specification0" xmi:type="uml:OpaqueExpression" body="succession = ownedMember->selectByKind(Succession)->at(1)" language="OCL2.0" name=""/>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-validateTransitionUsageParameters" xmi:type="uml:Constraint" name="validateTransitionUsageParameters">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-validateTransitionUsageParameters-_comment.0" xmi:type="uml:Comment" body="<p>A <code>TransitionUsage</code> must have at least one owned input <code>parameter</code> and, if it has a <code>triggerAction</code>, it must have at least two.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-validateTransitionUsageParameters"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-validateTransitionUsageParameters-specification0" xmi:type="uml:OpaqueExpression" body="if triggerAction->isEmpty() then inputParameters()->size() >= 1 else inputParameters()->size() >= 2 endif " language="OCL2.0" name=""/>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageTriggerAction" xmi:type="uml:Constraint" name="deriveTransitionUsageTriggerAction">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageTriggerAction-_comment.0" xmi:type="uml:Comment" body="<p>The <code>triggerActions</code> of a <code>TransitionUsage</code> are the <code>transitionFeatures</code> of the <code>ownedFeatureMemberships</code> of the <code>TransitionUsage</code> with <code>kind = trigger</code>, which must all be <code>AcceptActionUsages</code>.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-deriveTransitionUsageTriggerAction"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-deriveTransitionUsageTriggerAction-specification0" xmi:type="uml:OpaqueExpression" body="triggerAction = ownedFeatureMembership-> selectByKind(TransitionFeatureMembership)-> select(kind = TransitionFeatureKind::trigger).transitionFeature-> selectByKind(AcceptActionUsage)" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-TransitionUsage-checkTransitionUsageActionSpecialization" xmi:type="uml:Constraint" name="checkTransitionUsageActionSpecialization">
<constrainedElement xmi:idref="Systems-States-TransitionUsage"/>
<ownedComment xmi:id="Systems-States-TransitionUsage-checkTransitionUsageActionSpecialization-_comment.0" xmi:type="uml:Comment" body="<p>A composite <code>TransitionUsage</code> whose <code>owningType</code> is an <code>ActionDefinition</code> or <code>ActionUsage</code> and whose <code>source</code> is <em>not</em> a <code>StateUsage</code> must directly or indirectly specialize the <code>ActionUsage</code> <em><code>Actions::Action::decisionTransitions</code></em> from the Systems Model Library.</p>">
<annotatedElement xmi:idref="Systems-States-TransitionUsage-checkTransitionUsageActionSpecialization"/>
</ownedComment>
<specification xmi:id="Systems-States-TransitionUsage-checkTransitionUsageActionSpecialization-specification0" xmi:type="uml:OpaqueExpression" body="isComposite and owningType <> null and (owningType.oclIsKindOf(ActionDefinition) or owningType.oclIsKindOf(ActionUsage)) and source <> null and not source.oclIsKindOf(StateUsage) implies specializesFromLibrary('Actions::Action::decisionTransitions')" language="OCL2.0" name=""> </specification>
</ownedRule>
</packagedElement>
