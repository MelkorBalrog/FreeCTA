<annotatedElement xmi:idref="Systems-DefinitionAndUsage-VariantMembership-validateVariantMembershipOwningNamespace"/>
</ownedComment>
<specification xmi:id="Systems-DefinitionAndUsage-VariantMembership-validateVariantMembershipOwningNamespace-specification0" xmi:type="uml:OpaqueExpression" body="membershipOwningNamespace.oclIsKindOf(Definition) and membershipOwningNamespace.oclAsType(Definition).isVariation or membershipOwningNamespace.oclIsKindOf(Usage) and membershipOwningNamespace.oclAsType(Usage).isVariation " language="OCL2.0" name=""> </specification>
