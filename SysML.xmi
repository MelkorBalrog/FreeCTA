<specification xmi:id="Systems-States-StateUsage-deriveStateUsageDoAction-specification0" xmi:type="uml:OpaqueExpression" body="doAction = let doMemberships : Sequence(StateSubactionMembership) = ownedMembership-> selectByKind(StateSubactionMembership)-> select(kind = StateSubactionKind::do) in if doMemberships->isEmpty() then null else doMemberships->at(1) endif" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-StateUsage-validateStateUsageStateSubactionKind" xmi:type="uml:Constraint" name="validateStateUsageStateSubactionKind">
