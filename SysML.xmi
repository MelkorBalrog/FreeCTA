<ownedAttribute xmi:id="Systems-Occurrences-OccurrenceUsage-isIndividual" xmi:type="uml:Property" name="isIndividual">
<defaultValue xmi:id="Systems-Occurrences-OccurrenceUsage-isIndividual-defaultValue" xmi:type="uml:LiteralBoolean" name=""/>
<ownedComment xmi:id="Systems-Occurrences-OccurrenceUsage-isIndividual-_comment.0" xmi:type="uml:Comment" body="<p>Whether this <code>OccurrenceUsage</code> represents the usage of the specific individual represented by its <code>individualDefinition</code>.</p>">
<annotatedElement xmi:idref="Systems-Occurrences-OccurrenceUsage-isIndividual"/>
</ownedComment>
<type href="https://www.omg.org/spec/UML/20161101/PrimitiveTypes.xmi#Boolean"/>
