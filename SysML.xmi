<specification xmi:id="Systems-Requirements-RequirementUsage-deriveRequirementUsageRequiredConstraint-specification0" xmi:type="uml:OpaqueExpression" body="requiredConstraint = ownedFeatureMembership-> selectByKind(RequirementConstraintMembership)-> select(kind = RequirementConstraintKind::requirement). ownedConstraint" language="OCL2.0" name=""> </specification>
</ownedRule>
