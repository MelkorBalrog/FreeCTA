<constrainedElement xmi:idref="Systems-Views-ViewDefinition"/>
<ownedComment xmi:id="Systems-Views-ViewDefinition-validateViewDefinitionOnlyOneViewRendering-_comment.0" xmi:type="uml:Comment" body="<p>A <code>ViewDefinition</code> must have at most one <code>ViewRenderingMembership</code>.</p>">
