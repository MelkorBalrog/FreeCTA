</ownedComment>
<specification xmi:id="Systems-States-ExhibitStateUsage-checkExhibitStateUsageSpecialization-specification0" xmi:type="uml:OpaqueExpression" body="owningType <> null and (owningType.oclIsKindOf(PartDefinition) or owningType.oclIsKindOf(PartUsage)) implies specializesFromLibrary('Parts::Part::exhibitedStates')" language="OCL2.0" name=""> </specification>
</ownedRule>
<ownedRule xmi:id="Systems-States-ExhibitStateUsage-validateExhibitStateUsageReference" xmi:type="uml:Constraint" name="validateExhibitStateUsageReference">
