</ownedComment>
<specification xmi:id="Systems-DefinitionAndUsage-Usage-deriveUsageNestedCase-specification0" xmi:type="uml:OpaqueExpression" body="nestedCase = nestedUsage->selectByKind(CaseUsage)" language="OCL2.0" name=""/>
