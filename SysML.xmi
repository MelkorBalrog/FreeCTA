<ownedComment xmi:id="Systems-States-StateUsage-validateStateUsageParallelSubactions-_comment.0" xmi:type="uml:Comment" body="<p>If a <code>StateUsage</code> is parallel, then its <code>nestedActions</code> (which includes <code>nestedStates</code>) must not have any <code>incomingTransitions</code> or <code>outgoingTransitions</code>.</p>">
<annotatedElement xmi:idref="Systems-States-StateUsage-validateStateUsageParallelSubactions"/>
</ownedComment>
<specification xmi:id="Systems-States-StateUsage-validateStateUsageParallelSubactions-specification0" xmi:type="uml:OpaqueExpression" body="isParallel implies nestedAction.incomingTransition->isEmpty() and nestedAction.outgoingTransition->isEmpty()" language="OCL2.0" name=""/>
</ownedRule>
<ownedRule xmi:id="Systems-States-StateUsage-checkStateUsageExclusiveStateSpecialization" xmi:type="uml:Constraint" name="checkStateUsageExclusiveStateSpecialization">
