</ownedComment>
<specification xmi:id="Systems-Views-ViewDefinition-deriveViewDefinitionViewCondition-specification0" xmi:type="uml:OpaqueExpression" body="viewCondition = ownedMembership-> selectByKind(ElementFilterMembership). condition" language="OCL2.0" name=""/>
